  "scripts": {
    "prep": "npm install && npm i -g webpack-dev-server",
    "start-cabinet": "webpack-dev-server --mode development --env.type=cabinet --open",
    "start-site": "webpack-dev-server --mode development --env.type=site --open",
    "build-cabinet": "webpack --mode production --env.type=cabinet",
    "build-site": "webpack --mode production --env.type=site",
    "deploy-cabinet": "node deploy.js next/mobile2 cabinet",
    "deploy-site": "node deploy.js promo site",
    "flow-cabinet": "git pull && git push && npm run build-cabinet && npm run deploy-cabinet",
    "flow-site": "git pull && git push && npm run build-site && npm run deploy-site",
    "flow": "git pull && git push && npm run build-site && npm run deploy-site && npm run build-cabinet && npm run deploy-cabinet"
